/*
 * LEARNINGS
 * TO MAKE A BIT ZERO =&= ~(0x3 << (5*2));
 *
 */
#include <stdint.h>
#include "stm32f446rxx.h"



int main(void)
{

	GPIOA_CLK_EN();//Enable BUS for that - Enable GPIOA clock for that
	/*To access Moder Register (to set mode input or o/p)
	To make Moder as output we need to set as 0 1 according to Reference manual
	For that first make all as zero

	*/
	GPIOA->MODER &= ~(0x3 << 10); //to make 10th and 11 th bit 0
	GPIOA->MODER|=(1<<10); // MAKE 10 TH BIT 1

	//=============SET TYPE -PUSH PULL
	GPIOA->OTYPER &=~(1<<5);
	GPIOA->OSPEEDR &= ~(0x3 << 10);
	GPIOA->OSPEEDR|=(1<<10);

	GPIOA->PUPDR &= ~(0x3 << 10);
	GPIOA->ODR &=~(1<<5); // To Make LED on initially




	for(;;);
}
